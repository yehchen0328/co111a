/** 
 * Multiplexor:
 * out = a if sel == 1 
  */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel,out=nsel);    
    And(a=a,b=nsel,out=c);
    And(a=sel,b=b,out=d);
    Or(a=c,b=d,out=out);
}